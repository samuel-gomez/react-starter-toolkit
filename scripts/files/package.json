{
  "name": "my-name-application",
  "description": "This repository is a starter project that use React Toolkit Axa and CRA.",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cross-env GENERATE_SOURCEMAP=false react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "preview": "cross-env REACT_APP_NODE_ENV=production.local react-scripts build && serve -s build -p 5002",
    "test": "react-scripts test --watchAll=false",
    "test:sonar": "react-scripts test --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "cover:local": "npm t -- --coverage",
    "cover": "npm t -- --coverage --silent",
    "lint": "eslint ./src/ --ignore-pattern \"**/*.test.js\" --ignore-pattern \"**/*.steps.js\"",
    "lint:fix": "npm run lint --fix",
    "precommit": "npm run lint:fix && git add .",
    "prepare": "husky install",
    "cz": "cz",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "check": "npm run prettier && npm run lint"
  },
  "dependencies": {
    "@axa-fr/react-oidc-context": "5.14.2",
    "@axa-fr/react-toolkit-all": "1.4.1",
    "@axa-fr/react-toolkit-core": "1.4.1",
    "@babel/parser": "7.18.13",
    "@tanstack/react-query": "4.2.3",
    "@tanstack/react-query-devtools": "4.2.3",
    "@types/intl": "1.2.0",
    "@types/jest": "29.0.0",
    "@types/lodash": "4.14.184",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.3",
    "classnames": "2.3.1",
    "jest-cucumber": "3.0.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.5",
    "react-hook-form": "7.35.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.9",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "commitizen": "4.2.5",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.7",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "intl": "1.2.5",
    "jest-sonar-reporter": "2.0.0",
    "msw": "0.47.0",
    "precise-commits": "1.0.2",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "typescript": "4.8.2",
    "whatwg-fetch": "3.6.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "rootDir": "./",
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,mjs,ts,tsx}",
      "!<rootDir>/src/**/index.{js,ts,tsx}",
      "!<rootDir>/src/**/constants.{js,ts}",
      "!<rootDir>/src/**/*.stories.{js,ts}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/shared/testsUtils/**/*.{js,ts,tsx}",
      "!<rootDir>/src/**/__tests__/*.{js,ts}",
      "!<rootDir>/src/**/*.d.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "!**/__tests__/*.mock.{js,ts}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "clearMocks": true,
    "resetMocks": false
  },
  "resolutions": {
    "@babel/parser": "7.18.11"
  }
}
