{
  "name": "react-starter-toolkit",
  "description": "This repository is a starter project that use React Toolkit Axa and Next.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true npm run build",
    "preview": "next start",
    "test": "jest --watchAll=false",
    "cover:local": "npm t -- --coverage",
    "cover": "npm t -- --coverage --silent",
    "prepare": "husky install",
    "cz": "cz",
    "lint": "eslint ./src/ --ignore-pattern \"**/*.test.js\" --ignore-pattern \"**/*.steps.js\"",
    "lint:fix": "npm run lint --fix",
    "precommit": "npm run lint:fix && git add .",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "check": "npm run prettier && npm run lint",
    "check-types": "tsc --noEmit --pretty",
    "clean": "cd ./scripts && npm i && npm start && cd .. && npx rimraf ./scripts"
  },
  "dependencies": {
    "@axa-fr/react-oidc": "6.17.1",
    "@axa-fr/react-toolkit-all": "2.0.0",
    "@axa-fr/react-toolkit-core": "2.0.0",
    "@popperjs/core": "2.11.7",
    "@tanstack/react-query": "4.29.5",
    "@tanstack/react-query-devtools": "4.29.6",
    "@types/downloadjs": "1.4.3",
    "@types/lodash": "4.14.194",
    "@types/node": "20.1.0",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.4",
    "@uiw/react-textarea-code-editor": "2.1.1",
    "axe-core": "4.7.0",
    "cross-fetch": "3.1.5",
    "downloadjs": "1.4.7",
    "encoding": "0.1.13",
    "eslint-config-next": "13.4.1",
    "github-markdown-css": "5.2.0",
    "lodash": "4.17.21",
    "next": "13.4.1",
    "prism-react-renderer": "1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "4.4.5",
    "react-hook-form": "7.43.9",
    "react-json-view": "1.21.3",
    "react-jsx-parser": "1.29.0",
    "react-live": "2.4.1",
    "react-markdown": "8.0.5",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@next/bundle-analyzer": "13.4.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/testing-library__jest-dom": "5.14.5",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "intl": "1.2.5",
    "jest": "29.4.3",
    "jest-cucumber": "3.0.1",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "13.1.2",
    "msw": "1.0.1",
    "next-router-mock": "0.9.2",
    "prettier": "2.8.4",
    "sass": "1.62.1",
    "typescript": "5.0.4",
    "whatwg-fetch": "3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samuel-gomez/react-starter-toolkit.git"
  },
  "license": "MIT"
}
