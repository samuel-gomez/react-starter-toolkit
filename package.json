{
  "name": "react-starter-toolkit",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:local": "cross-env REACT_APP_NODE_ENV=production.local react-scripts build && serve -s build -p 5002",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test --watchAll=false",
    "test:sonar": "react-scripts test --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "cover": "cross-env NODE_ICU_DATA=node_modules/full-icu npm t -- --coverage --watchAll=false",
    "storybook": "start-storybook -p 9009 -s .storybook",
    "lint": "eslint ./src/ --ignore-pattern \"**/*.test.js\" --ignore-pattern \"**/*.steps.js\"",
    "lint:fix": "npm run lint --fix",
    "precommit": "npm run lint:fix && git add ."
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@axa-fr/react-oidc-context": "4.0.2",
    "@axa-fr/react-toolkit-all": "1.4.1",
    "@axa-fr/react-toolkit-core": "1.4.1",
    "@axa-fr/react-toolkit-modal-default": "1.3.23",
    "@babel/parser": "7.17.3",
    "@storybook/addon-a11y": "6.4.19",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-options": "5.3.21",
    "@storybook/addon-storysource": "6.4.19",
    "@storybook/addon-viewport": "6.4.19",
    "@storybook/addons": "6.4.19",
    "@storybook/core": "6.4.19",
    "@storybook/react": "6.4.19",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "5.1.3",
    "@testing-library/user-event": "12.8.3",
    "apexcharts": "3.33.1",
    "classnames": "2.3.1",
    "downloadjs": "1.4.7",
    "jest-cucumber": "3.0.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mw.validation": "1.0.15",
    "prism-react-renderer": "1.3.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-apexcharts": "1.3.9",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-github-login": "1.0.3",
    "react-google-login": "5.2.2",
    "react-live": "2.4.1",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-social-login": "3.4.15",
    "intl": "1.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "10.0.0",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-flowtype": "5.10.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "full-icu": "1.4.0",
    "husky": "4.3.8",
    "jest-sonar-reporter": "2.0.0",
    "precise-commits": "1.0.2",
    "prettier": "2.5.1",
    "react-test-renderer": "16.14.0",
    "sass": "1.49.9"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "rootDir": "./",
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx,mjs}",
      "!<rootDir>/src/**/index.js",
      "!<rootDir>/src/**/constants.js",
      "!<rootDir>/src/**/*.stories.js",
      "!<rootDir>/src/**/*.rules.js",
      "!<rootDir>/src/shared/hoc/withEnvAndAuth.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/shared/helpers/compose.js",
      "!<rootDir>/src/testsUtils/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "clearMocks": true,
    "resetMocks": false
  },
  "resolutions": {
    "@babel/parser": "7.17.3"
  }
}
